<!--pages/tiaosheng/tiaosheng.wxml-->
<view class = 'bac'>
<audio  name="{{name}}" src="{{src}}" id = 'myAudio' controls loop></audio>

<button type="primary" bindtap="audioPlay" class = 'style'>播放</button>
<button type="primary" bindtap="audioPause" class = 'style'>暂停</button>
<button type="primary" bindtap="audio14" class = 'style'>设置当前播放时间为14秒</button>
<button type="primary" bindtap="audioStart" class = 'style'>回到开头</button>
</view>